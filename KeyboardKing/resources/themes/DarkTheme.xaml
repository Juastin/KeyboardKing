<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- COLOR SCHEMES -->
    <SolidColorBrush x:Key="Color1" Color="{DynamicResource SColor1}"/>
    <SolidColorBrush x:Key="Color2" Color="{DynamicResource SColor2}"/>
    <SolidColorBrush x:Key="Color3" Color="{DynamicResource SColor3}"/>
    <SolidColorBrush x:Key="Color4" Color="{DynamicResource SColor4}"/>
    <SolidColorBrush x:Key="Color5" Color="{DynamicResource SColor5}"/>

    <!-- MAIN COLORS -->
    <SolidColorBrush x:Key="TextColor" Color="{DynamicResource SText}"/>
    <SolidColorBrush x:Key="BackgroundColor" Color="{DynamicResource SBackground}"/>
    <SolidColorBrush x:Key="BorderColor" Color="{DynamicResource SBorder}"/>

    <!-- Dark Theme -->
    <Color x:Key="SText">White</Color>
    <Color x:Key="SBackground">#011124</Color>
    <Color x:Key="SBorder">White</Color>

    <!-- Tints -->
    <SolidColorBrush x:Key="Color6" Color="{DynamicResource SColor6}"/>
    <SolidColorBrush x:Key="Color7" Color="{DynamicResource SColor7}"/>

    <Color x:Key="SColor1">#000814</Color>
    <Color x:Key="SColor2">#001D3D</Color>
    <Color x:Key="SColor3">#003566</Color>
    <Color x:Key="SColor4">#FFC300</Color>
    <Color x:Key="SColor5">#FFCF33</Color>
    <Color x:Key="SColor6">#FFFFFF</Color>
    <Color x:Key="SColor7">#bdbdbd</Color>
</ResourceDictionary>